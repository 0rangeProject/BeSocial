<?xml version="1.0" encoding="utf-8"?>
<!--suppress ALL -->
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.agathe.tsgtest"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-sdk tools:overrideLibrary="com.olab.dns_library.dnslibrary"/>
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-feature android:name="@string/step_detector" android:required="true"/>
    <uses-feature android:name="@string/step_counter" android:required="true"/>
    <uses-feature android:name="android.hardware.location" android:required="true" />
    <uses-feature android:name="android.hardware.location.gps" android:required="false" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.USE_CREDENTIALS" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" />
    <uses-permission android:name="com.example.agathe.tsgtest.permission.C2D_MESSAGE" />

    <permission
        android:name="com.example.agathe.tsgtest.permission.C2D_MESSAGE"
        android:protectionLevel="signature" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyA8J1zvgt2LI6RX23xxboJGVB8ad2ONir4" />


        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".FirstSportActivity"
            android:label="SPORT"
            android:theme="@style/AppTheme" >
        </activity>

        <activity
            android:name=".sport.ChallengeActivity"
            android:label="SPORT"
            android:theme="@style/AppTheme" >
        </activity>

        <activity
            android:name=".sport.LittleServicesActivity"
            android:label="Orange Little Services"
            android:theme="@style/AppTheme" >
        </activity>

        <activity android:name=".carpooling.PotentialCarpoolersActivity"></activity>
        <activity android:name=".carpooling.ServerMainActivity"></activity>
        <activity android:name=".carpooling.AuthorizationActivity"></activity>
        <activity android:name=".SignInActivity"></activity>
        <activity android:name=".SplashActivity"></activity>
        <activity android:name=".carpooling.MapsActivity" />
        <activity
            android:name=".carpooling.PurposeActivity"
            android:label="@string/title_activity_purpose"
            android:theme="@style/AppTheme"></activity>
        <activity
            android:name=".events.PublicEventsActivity"
            android:parentActivityName=".MainActivity"></activity>

        <receiver
            android:name="com.google.android.gms.gcm.GcmReceiver"
            android:exported="true"
            android:permission="com.google.android.c2dm.permission.SEND" >
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />
                <category android:name="com.example.agathe.tsgtest" />
            </intent-filter>
        </receiver>

        <!-- BEGIN - PUSH NOTIFICATIONS WITH GOOGLE CLOUD MESSAGING (GCM) -->
        <service
            android:name=".PushListenerService"
            android:exported="false" >
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />
            </intent-filter>
        </service>
        <!-- END - PUSH NOTIFICATIONS WITH GOOGLE CLOUD MESSAGING (GCM) -->
    </application>

</manifest>